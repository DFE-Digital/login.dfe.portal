<a href="/" class="link-back">Back</a>
<div class="grid-row">
    <div class="col-8">
        <h1 class="heading-xlarge">
            All services within DfE sign in
        </h1>
        <% if(locals.availableServices) { %>
        <ul class="list-bullet available-service-list">
            <% for (let i = 0; i < locals.availableServices.length; i++) { %>
            <li>
                <h3 class="heading-small"><%=locals.availableServices[i].name%></h3>
                <a href="services/request-access/<%=locals.availableServices[i].id%>">
                    Request access to <%=locals.availableServices[i].name%>
                </a>
            </li>
            <% } %>
        </ul>
        <% } else { %>
        <p>No services available</p>
        <% } %>
    </div>
</div>
